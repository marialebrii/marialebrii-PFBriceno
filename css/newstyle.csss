@import url("https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;1,200&display=swap");
* {
  margin-left: 20px;
  margin-right: 20px;
  padding: 0;
  box-sizing: border-box;
}

html {
  font-size: 62.5%;
  font-family: "Poppins", sans-serif;
}

img {
  width: 100%;
}

.logo {
  width: 100px;
  height: auto;
}

ul {
  list-style: none;
}

a {
  text-decoration: none;
  color: #3d1c35;
  font-size: 2rem;
}

